{
    "name": "rtlive", 
    "indexes":[
        ["date","region"]
    ],
    "fields": {
        "date": {
            "type": "timestamp",
            "format": "YYYY-MM-DD"
        },
        "region": {
            "type": "string",
            "maxLength": 2
        },
        "mean": {
            "type": "double"
        },
        "median": {
            "type": "double"
        },
        "lower_80": {
            "type": "double"
        },
        "upper_80": {
            "type": "double"
        },
        "new_cases": {
            "type": "int"
        },
        "new_deaths": {
            "type": "int"
        },
        "new_tests": {
            "type": "int"
        },
        "hash": {
            "type": "string",
            "maxLength": 50,
            "primary_key": true
        }
    }
}